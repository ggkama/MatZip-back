<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kh.matzip.notice.model.dao.NoticeMapper">

    <select id="selectNoticeList" resultType="int">
        SELECT
            COUNT(*)
        FROM
            TB_USER
        WHERE
            userId = #{userId}
    </select>

    <select id="existByUserNickName" resultType="int">
        SELECT
            COUNT(*)
        FROM
            TB_USER
        WHERE
            userNickName = #{userNickName}
    </select>

    <insert>
    </insert>

</mapper>